list1 = [8, 9, 148, 4 ]
list2 = [9, 1, 33, 83]
finalList = [] 
n = 0
for i in list1:
    for j in list2:
        finalList.append(i*j)
        

    
    
    
list1 = [8, 9, 148, 4 ]
list2 = [9, 1, 33, 83]
sdf = []
for i in range(0, len(list1)):
    sdf.append(list1[i] * list2[i])

print(finalList + sdf)